{
	"name": "fortnite-store-spy",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "node ./bin/www",
		"build": "webpack --mode production",
		"client-dev": "webpack --mode development --watch --hot",
		"server-dev": "nodemon ./bin/www",
		"dev": "concurrently \"npm run server-dev\" \"npm run client-dev\"",
		"test": "jest"
	},
	"jest": {
		"moduleNameMapper": {
			"^.+\\.(css|scss)$": "identity-obj-proxy"
		},
		"setupTestFrameworkScriptFile": "./src/setupTests.js"
	},
	"dependencies": {
		"axios": "^0.18.0",
		"bootstrap": "^4.1.3",
		"concurrently": "^3.6.1",
		"cookie-parser": "~1.4.3",
		"debug": "~2.6.9",
		"express": "~4.16.0",
		"mongoose": "^5.2.5",
		"morgan": "~1.9.0",
		"node-sass-middleware": "0.11.0",
		"prop-types": "^15.6.2",
		"react": "^16.4.1",
		"react-dom": "^16.4.1",
		"react-redux": "^5.0.7",
		"redux": "^4.0.0",
		"redux-thunk": "^2.3.0"
	},
	"devDependencies": {
		"babel-loader": "^7.1.5",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"css-loader": "^1.0.0",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"html-webpack-plugin": "^3.2.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^23.4.1",
		"nodemon": "^1.18.3",
		"sass-loader": "^7.0.3",
		"style-loader": "^0.21.0",
		"webpack": "^4.16.3",
		"webpack-cli": "^3.1.0"
	}
}
